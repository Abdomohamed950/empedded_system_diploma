#include <stdint.h>
#include <stdlib.h>
#include <uart_driver.h>



void uart_init(UART_CONF* conf)
{
	USART_R* usart = conf.;
	if(conf->USARTx == USART1)
	{
		RCC_USART1_CLK_EN();

	}
	else if(conf->USARTx == USART2)
	{
		RCC_USART2_CLK_EN();

	}
	else if(conf->USARTx == USART3)
	{
		RCC_USART3_CLK_EN();

	}


}

